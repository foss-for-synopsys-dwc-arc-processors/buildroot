start() {
#	printf '%s %s: ' "$1" "$PROGRAM"
#	status=$("$run_program" 4>&1)
#	echo "$status"
#	if [ "$status" = "OK" ]; then
#		return 0
#	fi
	zcat /proc/config.gz | egrep "(RAMFS_SO|TUNE|DIV|LL128)"
	return 0
}

case "$1" in
	start)
		start "";;
	restart|reload)
		start "Rerunning";;
	stop)
		:;;
	*)
		echo "Usage: $0 {start|stop|restart|reload}"
		exit 1
esac
